    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "attach",
            "name": "Attach to PID",
            "gdbpath": "/opt/riscv/bin/riscv32-unknown-linux-gnu-gdb",
            "target": ":1235",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",
            "executable": "app/hello/hello",
            "remote": true
        }
    ]
}
